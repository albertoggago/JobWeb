		{"pages":[
			{"name":"linkedin",
			 "ruleFind":"linkedin",
			 "necesaryVariables" : ["titulo","donde","summary","fecha","company"],
			 "rulesTransformUrl" : [],
			 "rulesTransformData" : {
			  	"titulo" : {"secuences": [{"tipo":"class","elemento":"title"}]},
			  	"donde"  : {"secuences": [{"tipo":"class","elemento":"location"}]},
				"summary": {"secuences": [{"tipo":"class","elemento":"summary"}]},
				"fecha"  : {"secuences": [{"tipo":"class","elemento":"posted"}], 
				            "out"     :  {"tipo":"fecha-dif"}},
				"company": {"secuences": [{"tipo":"class","elemento":"company"}]}
			   },
			"rulestransformFinal": [{"in":"titulo",
									 "out":"titulo",
			                         "valueIn":"Destaca en lo que haces, Consultor - Cient√≠fico de Datos - Scrum Master",
			                     	 "action":"SPACES"},
			                     	{"in":"company",
									 "out":"company",
			                         "valueIn":"",
			                     	 "action":"COPY",
			                     	 "valueOut":"<NO DEFINIDA>"}],
			"rulesOkFinding": ["titulo","donde","summary","company"]
			 },
			{"name":"irishjobs",
			 "ruleFind":"irishjobs",
			 "necesaryVariables" : ["titulo","donde","summary","fecha","company",
			 	                    "company2","salary","jobType"],
			 "rulesTransformUrl" : [],
			 "rulesTransformData" : {
			  	"titulo" : {"secuences": [{"tipo":"class", "elemento":"job-description"}, 
			  		                      {"tipo":"tag", "elemento":"h1"}]},
			  	"donde"  : {"secuences": [{"tipo":"class", "elemento":"location"}]},
				"summary": {"secuences": [{"tipo":"class", "elemento":"job-details"}]},
				"fecha"  : {"secuences": [{"tipo":"class", "elemento":"updated-time"}],
				            "split":{"text_split":" ", "n":1, "initText":null}, 
				            "out"     :  {"tipo":"fecha", "formato":"%d/%m/%Y"}},
				"company": {"secuences": [{"tipo":"class", "elemento":"job-description"}, 
					                      {"tipo":"tag", "elemento":"h2"}]},
				"salary": {"secuences": [{"tipo":"class", "elemento":"salary"}]},
			    "jobType": {"secuences": [{"tipo":"class", "elemento":"employment-type"}]}
			   },
			"rulestransformFinal": [{"in":"company",
									 "out":"company",
			                         "valueIn":"",
			                     	 "action":"COPY-ANOTHER",
			                     	 "another":"titulo"}],
			"rulesOkFinding": ["titulo","donde","summary","company"]
			 },
			 {"name":"recruitireland",
			 "ruleFind":"recruitireland",
			 "necesaryVariables" : ["titulo","donde","summary","fecha","company",
			 	                    "company2","salary","jobType"],
			 "rulesTransformUrl" : [{"from":"=2E", "to":"."},{"from":"=3D", "to":"="}],
			 "rulesTransformData" : {
			  	"titulo" : {"secuences": [{"tipo":"class", "elemento":"job-header"}],
			             "split":{"text_split":"\n", "n":0, "initText":null}},
			  	"donde"  : {"secuences": [{"tipo":"class", "elemento":"job-info"}],
			                "split": {"text_split":"\n", "n":0, "initText":"Location:"}},
				"summary": {"secuences": [{"tipo":"class", "elemento":"generic-text"}]},
				"fecha"  : {"secuences": [{"tipo":"class", "elemento":"job-info"}],
				            "split":{"text_split":"\n", "n":0, "initText":"Posted on:"}, 
				            "out"     :  {"tipo":"fecha", "formato":"%d-%b-%Y"}},
				"company": {"secuences": [{"tipo":"class","elemento":"company"}]},
			    "company2": {"secuences": [{"tipo":"class", "elemento":"job-owner-description"},
			    	                       {"tipo":"tag", "elemento":"a"}]},
				"salary": {"secuences": [{"tipo":"class", "elemento":"job-info"}],
			                "split": {"text_split":"\n", "n":0, "initText":"Salary:"}},
			    "jobType": {"secuences": [{"tipo":"class", "elemento":"job-info"}],
			                "split": {"text_split":"\n", "n":0, "initText":"Job type:"}}
			   },
			"rulestransformFinal": [{"in":"titulo",
									 "out":"titulo",
			                         "valueIn":"THIS JOB HAS BEEN REMOVED BY THE ADVERTISER.",
			                     	 "action":"SPACES"},
			                     	 {"in":"company",
									 "out":"company",
			                         "valueIn":"",
			                     	 "action":"COPY-ANOTHER",
			                     	 "another":"company2"},
			                     	 {"in":"titulo",
									 "out":"titulo",
			                         "valueIn":"INCORRECT JOB ID NUMBER",
			                     	 "action":"SPACES"}
			                     	],
			"rulesOkFinding": ["titulo","donde","summary","company"]
			 },			
			 {"name":"jobs.ie",
			 "ruleFind":".jobs.ie",
			 "necesaryVariables" : ["titulo","donde","summary","fecha","company",
			 	                    "salary","jobType"],
			 "rulesTransformUrl" : [{"from":"=3D", "to":"="}],
			 "rulesTransformData" : {
			  	"titulo" : {"secuences": [{"tipo":"class", "elemento":"padding"}, 
			  		                      {"tipo":"tag", "elemento":"h1"}],
			             "split":{"text_split":"\\u00e2\\0080\\0093", "initText":null, "n":0}},
			  	"donde"  : {"secuences": [{"tipo":"class", "elemento":"c1"}],
			                "split": {"text_split":"\n", "initText":null, "n":1}},
				"summary": {"secuences": [{"tipo":"class", "elemento":"job-description"}]},
				"fecha"  : {"secuences": [{"tipo":"class", "elemento":"c2"}],
				            "split":{"text_split":"\n", "n":3, "initText":null}, 
				            "out"     :  {"tipo":"fecha", "formato":"%d-%m-%Y"}},
				"company": {"secuences": [{"tipo":"class", "elemento":"sidebar"}, 
					                      {"tipo":"class", "elemento":"acc-panel"}],
			                 "split":{"text_split":"\n", "initText":null, "n":1}},
			    "company2": {"secuences": [{"tipo":"class", "elemento":"job-owner-description"},
			    	                       {"tipo":"tag", "elemento":"a"}]},
				"salary": {"secuences": [{"tipo":"class", "elemento":"c1"}],
			                "split": {"text_split":"\n", "initText":"Payment:", "n":0}},
			    "jobType": {"secuences": [{"tipo":"class", "elemento":"c2"}],
			                "split": {"text_split":"\n", "initText":"Terms:", "n":0}}
			   },
			"rulestransformFinal": [],
			"rulesOkFinding": ["titulo","donde","summary","company"]
			 },
			 {"name":"monster.ie",
			 "ruleFind":".monster.",
			 "necesaryVariables" : ["titulo","donde","summary","fecha","company",
			 	                    "salary","jobType"],
			 "rulesTransformUrl" : [{"from":"=3D", "to":"="}],
