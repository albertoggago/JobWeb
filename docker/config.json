			  	"titulo" : {"secuences": [{"tipo":"class", "elemento":"title"}],
			             "split":{"text_split":" - ", "n":0, "initText":null}},
			  	"donde"  : {"secuences": [{"tipo":"class", "elemento":"subtitle"}]},
				"summary": {"secuences": [{"tipo":"class", "elemento":"details-content"}]},
				"fecha"  : {"secuences": [{"tipo":"class", "elemento":"mux-job-summary"}],
				            "split":{"text_split":"\n", "n":0, "initText":"Posted"}, 
				            "out"     :  {"tipo":"fecha-dif"}},
				"company": {"secuences": [{"tipo":"class", "elemento":"title"}],
				            "split":{"text_split":" - ", "n":1, "initText":null}}
			    },
			"rulestransformFinal": [{"in":"titulo",
									 "out":"titulo",
			                         "valueIn":"Does your CV pass the 6-second test?",
			                     	 "action":"SPACES"}
			                     	],
			"rulesOkFinding": ["titulo","donde","summary","company"]
